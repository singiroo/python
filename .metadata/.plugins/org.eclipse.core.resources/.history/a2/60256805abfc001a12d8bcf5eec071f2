from random import *
gawi = ["가위","바위","보"]
com=""
mine = ""
res = ""
i = randint(0,2)
j = randint(0,2)

com = gawi[i]
mine = gawi[j]

if(com == "가위" and mine =="보") or (com=="바위" and mine=="가위" ) or(com == "보"and mine="바위"):
    res = "졌습니다"
elif(com == mine):
    res = "비겼습니다"
else:
    res = "이겼습니다"

print(res)
print("나 : {}   컴 : {}".format(mine,com))
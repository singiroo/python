package Test;

public class CallbyRef {
	
	
	
	//a => value
	public static int add(int a) {
		return a++;
	}
	
	public static int[] addRef(int[] a) {
		a[0]++;
		return a;
	}
	
	
	
	
	public static void main(String[] args) {
		int a =1;
		int[] b = {3};
		
		int c = add(a);
		int[] d = addRef(b);
		
		
		
		
	}
	
	
	
	
	
}

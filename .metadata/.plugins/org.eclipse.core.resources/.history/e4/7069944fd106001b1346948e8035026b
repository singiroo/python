import cx_Oracle

print("oracle")

conn = cx_Oracle.connect("KYH/java@localhost:1521/xe")
cursor = conn.cursor()
sql = "insert into buyer values(:1,:2,:3)"
data = ("P10101","한성컴퓨터 ","P101")
cursor.execute(sql,data)

for row in cursor :
    for i in range(len(row)): 
        print(row[i],end="\t")
        
        
    print()
    
cursor.close()
conn.close()
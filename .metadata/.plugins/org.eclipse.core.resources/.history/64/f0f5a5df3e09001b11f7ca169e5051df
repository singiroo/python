package day11;

public class ThreadTest03 {
	public static void main(String[] args) {
		
		
		
		
		
	}
	public static void printNumber() {
		new Thread() {
			@Override
			public void run() {
				for(int i=0; i<100000; i++) {
					System.out.print(i);
					if(i%100 == 0) {
						System.out.println();
					}
				}
			}
		}.start();
	}
	
	public static void printChar() {
		new Thread() {
			@Override
			public void run() {
				for(int i=0; i<100000; i++) {
					System.out.print((char)(i));
					if(i%100 == 0) {
						System.out.println();
					}
				}
			}
		}.start();
	}
}
